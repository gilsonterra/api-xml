{
    "openapi": "3.0.0",
    "info": {
        "title": "Import XML API",
        "license": {
            "name": "MIT"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost/api",
            "description": "Server API Rest"
        }
    ],
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "Login"
                ],
                "summary": "get token",
                "operationId": "App\\Http\\Controllers\\LoginController::index",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Token",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "schema": "Token",
                                    "title": "Token",
                                    "description": "User Token",
                                    "properties": {
                                        "user": {
                                            "$ref": "#/components/schemas/User"
                                        },
                                        "token": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/people": {
            "get": {
                "tags": [
                    "People"
                ],
                "summary": "Get all people",
                "operationId": "App\\Http\\Controllers\\PeopleController::all",
                "responses": {
                    "200": {
                        "description": "Person",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/people/{id}": {
            "get": {
                "tags": [
                    "People"
                ],
                "summary": "Get person by id",
                "operationId": "App\\Http\\Controllers\\PeopleController::show",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Person ID",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Person",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/shiporders": {
            "get": {
                "tags": [
                    "ShipOrder"
                ],
                "summary": "Get all shiporders",
                "operationId": "App\\Http\\Controllers\\ShipordersController::all",
                "responses": {
                    "200": {
                        "description": "ShipOrder",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/shiporders/{id}": {
            "get": {
                "tags": [
                    "ShipOrder"
                ],
                "summary": "Get shiporder by id",
                "operationId": "App\\Http\\Controllers\\ShipordersController::show",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ShipOrder ID",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ShipOrder",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Person": {
                "title": "Person",
                "description": "Person Imported",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "phones": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Phone"
                        }
                    },
                    "shiporders": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Shiporder"
                        }
                    }
                },
                "type": "object"
            },
            "Phone": {
                "title": "Phone",
                "description": "Phone of person",
                "properties": {
                    "number": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "Shiporder": {
                "title": "ShipOrder",
                "description": "Ship Order from Person",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "person": {
                        "$ref": "#/components/schemas/Person"
                    },
                    "shipto_address": {
                        "type": "string"
                    },
                    "shipto_name": {
                        "type": "string"
                    },
                    "shipto_city": {
                        "type": "string"
                    },
                    "shipto_country": {
                        "type": "string"
                    },
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Items"
                        }
                    }
                },
                "type": "object"
            },
            "Items": {
                "title": "Items",
                "description": "Items of shiporder",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "note": {
                        "type": "string"
                    },
                    "quantity": {
                        "type": "integer"
                    },
                    "price": {
                        "type": "float"
                    }
                },
                "type": "object"
            },
            "User": {
                "title": "User",
                "description": "User login",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearer_token": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    },
    "security": [
        []
    ]
}